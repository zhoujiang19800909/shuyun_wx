/** 修改默认的navigator点击态 **/

.home_text_menu {
  color:#aaa;font-size: 12px;
}

.home_menu {
  width: 64rpx;height: 64rpx;
}

.flex_row {
  display: flex;flex-direction: row;align-items: center;background-color: #ffffff;margin-bottom: 10rpx;
}

.flex_col {
  display: flex;flex-direction: column;align-items: center;background-color: #ffffff;
